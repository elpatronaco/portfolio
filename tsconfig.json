{
  "compilerOptions": {
    "allowJs": true,
    "allowUnreachableCode": false,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "baseUrl": ".",
    "target": "ESNext",
    "noImplicitAny": true,
    "incremental": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "noEmit": true
  },
  "exclude": ["node_modules"],
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"]
}
